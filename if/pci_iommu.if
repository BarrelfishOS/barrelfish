interface pci_iommu "The Iommu to PCI interface" {

    rpc get_iommu_endpoint(in uint32 seg,        /* PCI segment */
                           in uint32 bus,        /* PCI bus */
                           in uint32 device,     /* PCI device */
                           in uint32 function,   /* PCI function */
                           out cap ep,
                           out errval err);
};
