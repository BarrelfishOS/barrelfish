FILES="../icparc_solver/ic.c ../icparc_solver/bitmap.c ../icparc_solver/edge_finder.c"
echo "
#include <dlfcn.h>
"

for f in $FILES; do
#    sed -r -n -e 's/(^[a-zA-Z0-9_]+)\(.*\).*/extern void \1();/p' $f
    sed -r -n -e 's/(^[a-zA-Z0-9_]+)\(.*/extern void \1();/p' $f
done


echo "
struct function_entry funcs[] = {
"

for f in $FILES; do
#    sed -r -n -e 's/(^[a-zA-Z0-9_]+)\(.*\).*/{.name="\1",.f=\1},/p' $f
    sed -r -n -e 's/(^[a-zA-Z0-9_]+)\(.*/{.name="\1",.f=\1},/p' $f
done

echo "};"

