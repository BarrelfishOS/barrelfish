if test "$cplex_versions_requested" != "no"; then
    if test "$cplex_versions_requested" = "yes"; then
    	cplex_versions_requested="75 81 90 102 110 121 122 1251"
    fi
    for version in $cplex_versions_requested; do
	CPLEXDIR="$ECLIPSETHIRDPARTY/cplex$version"
	if test -d $CPLEXDIR/lib/$ARCH -o \
		-d $CPLEXDIR/lib/$CPLEX_ARCH_PREFIX -o \
		-d $CPLEXDIR/bin/$CPLEX_ARCH_PREFIX >/dev/null 2>&1 ; then
	    CPLEX_VERSIONS="$CPLEX_VERSIONS $version"
	    { $as_echo "$as_me:${as_lineno-$LINENO}: Using Cplex in $CPLEXDIR" >&5
$as_echo "$as_me: Using Cplex in $CPLEXDIR" >&6;}
	else
	    { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Could not find $ARCH support in $CPLEXDIR" >&5
$as_echo "$as_me: WARNING: Could not find $ARCH support in $CPLEXDIR" >&2;}
	fi
    done
fi
if test -n "$CPLEX_VERSIONS"; then
    { $as_echo "$as_me:${as_lineno-$LINENO}: Will build Cplex interfaces for versions $CPLEX_VERSIONS" >&5
$as_echo "$as_me: Will build Cplex interfaces for versions $CPLEX_VERSIONS" >&6;}
    INSTALL_EPLEX=install_eplex
else
    { $as_echo "$as_me:${as_lineno-$LINENO}: Not building any Cplex interfaces" >&5
$as_echo "$as_me: Not building any Cplex interfaces" >&6;}
fi


