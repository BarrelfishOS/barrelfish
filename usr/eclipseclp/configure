    if test "$osi_versions_requested" = "yes"; then
    	osi_versions_requested="clpcbc symclp"
    fi
    for version in $osi_versions_requested; do
	found_osi_version=
		OSIDIRS="$OSIBASEDIR${version}amd $OSIBASEDIR$version"
	for OSIDIR in $OSIDIRS; do
	    if test -d "$OSIDIR/$ARCH/lib"; then
		found_osi_version=yes
		OSI_VERSIONS="$OSI_VERSIONS $version"
		{ $as_echo "$as_me:${as_lineno-$LINENO}: Using OSI $version in $OSIDIR" >&5
$as_echo "$as_me: Using OSI $version in $OSIDIR" >&6;}
		case $version in
		clpcbc) CLPCBCDIR="$OSIDIR" ;;
		symclp) SYMCLPDIR="$OSIDIR" ;;
		glpk)  OSIGLPKDIR="$OSIDIR" ;;
		esac
		ls -d $OSIDIR/$ARCH/lib/*amd* >/dev/null 2>/dev/null
		if test $? -eq 0; then
		     { $as_echo "$as_me:${as_lineno-$LINENO}: Found AMD support for $version" >&5
$as_echo "$as_me: Found AMD support for $version" >&6;}
		     OSICLP_ADDLIB="-lamd"
		     OSICLP_ADDDEF="-DUFL_BARRIER"
		fi
		break
	    fi
	done
	if test -z "$found_osi_version"; then
	    { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Could not find $ARCH support for $version in $OSIDIRS" >&5
$as_echo "$as_me: WARNING: Could not find $ARCH support for $version in $OSIDIRS" >&2;}
	fi
    done
fi
if test -n "$OSI_VERSIONS"; then
    { $as_echo "$as_me:${as_lineno-$LINENO}: Will build OSI interfaces for versions $OSI_VERSIONS" >&5
$as_echo "$as_me: Will build OSI interfaces for versions $OSI_VERSIONS" >&6;}
