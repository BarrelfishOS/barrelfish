# Extract the first word of "latex", so it can be a program name with args.
set dummy latex; ac_word=$2
{ $as_echo "$as_me:${as_lineno-$LINENO}: checking for $ac_word" >&5
$as_echo_n "checking for $ac_word... " >&6; }
if ${ac_cv_prog_PROG_LATEX+:} false; then :
  $as_echo_n "(cached) " >&6
else
  if test -n "$PROG_LATEX"; then
  ac_cv_prog_PROG_LATEX="$PROG_LATEX" # Let the user override the test.
else
as_save_IFS=$IFS; IFS=$PATH_SEPARATOR
for as_dir in $PATH
do
  IFS=$as_save_IFS
  test -z "$as_dir" && as_dir=.
    for ac_exec_ext in '' $ac_executable_extensions; do
  if as_fn_executable_p "$as_dir/$ac_word$ac_exec_ext"; then
    ac_cv_prog_PROG_LATEX="yes"
    $as_echo "$as_me:${as_lineno-$LINENO}: found $as_dir/$ac_word$ac_exec_ext" >&5
    break 2
  fi
done
  done
IFS=$as_save_IFS

  test -z "$ac_cv_prog_PROG_LATEX" && ac_cv_prog_PROG_LATEX="no"
fi
fi
PROG_LATEX=$ac_cv_prog_PROG_LATEX
if test -n "$PROG_LATEX"; then
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: $PROG_LATEX" >&5
$as_echo "$PROG_LATEX" >&6; }
else
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
fi


