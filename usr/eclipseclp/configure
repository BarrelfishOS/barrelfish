JAVA_SEARCH_PATH="\
$ECLIPSETHIRDPARTY/java/$ARCH/java
/usr/java
/usr/lib/java
/usr/lib/jvm
/usr/lib/jvm/java
/usr/lib/jvm/default-java
/usr/j2se
C:/Program Files/Java/jdk*
$PROGRAMFILES/Java/jdk*
/Library/Java/Home"


# Check whether --with-java was given.
if test "${with_java+set}" = set; then :
  withval=$with_java; with_java="$withval"
else
  with_java=yes
fi


if test "$with_java" = "no"; then
    unset JAVA_HOME
    unset JRE_HOME
    ALL_WITH_JAVA=
    ALL_CROSS_WITH_JAVA=
    { $as_echo "$as_me:${as_lineno-$LINENO}: Not building Java interface" >&5
$as_echo "$as_me: Not building Java interface" >&6;}
else
    if test "$with_java" != "yes"; then
	JAVA_SEARCH_PATH="$with_java"
    elif test -n "$JAVA_HOME"; then
	JAVA_SEARCH_PATH="$JAVA_HOME"
    fi
    IFSorig=$IFS
    IFS=`printf '\n\t'`
    for JAVA_HOME in $JAVA_SEARCH_PATH; do
	if test -x "$JAVA_HOME/bin/javac" -a -d "$JAVA_HOME/include"; then
	    break
	fi
	JAVA_HOME=
    done
    IFS=$IFSorig
    if test -n "$JAVA_HOME"; then
	$as_echo "#define HAVE_JAVA 1" >>confdefs.h

	ALL_WITH_JAVA=all_with_java
	ALL_CROSS_WITH_JAVA=all_cross_with_java
	{ $as_echo "$as_me:${as_lineno-$LINENO}: Using Java in $JAVA_HOME" >&5
$as_echo "$as_me: Using Java in $JAVA_HOME" >&6;}
    else
	unset JAVA_HOME
	ALL_WITH_JAVA=
	ALL_CROSS_WITH_JAVA=
	{ $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Could not find a usable Java installation" >&5
$as_echo "$as_me: WARNING: Could not find a usable Java installation" >&2;}
    fi
fi


