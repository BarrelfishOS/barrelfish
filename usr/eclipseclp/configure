    GRAPPA_REQUIRED="1.2"
    for version in $GRAPPA_REQUIRED; do
	GRAPPA_DIR=$ECLIPSETHIRDPARTY/grappa$version
	GRAPPA_JAR=grappa`echo $version|tr . _`.jar
	if test -r $GRAPPA_DIR/$GRAPPA_JAR; then
	    break
	fi
	GRAPPA_JAR=
    done
    if test -z "$GRAPPA_JAR"; then
	{ $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Could not find Grappa ($GRAPPA_REQUIRED) in $ECLIPSETHIRDPARTY" >&5
$as_echo "$as_me: WARNING: Could not find Grappa ($GRAPPA_REQUIRED) in $ECLIPSETHIRDPARTY" >&2;}
    else
        if test -z "$JRE_HOME"; then
	   if test -d $JAVA_HOME/jre; then
	      JRE_HOME=$JAVA_HOME/jre
	   else
	      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Could not find JRE for Java; Visualisation tools will not be built." >&5
$as_echo "$as_me: WARNING: Could not find JRE for Java; Visualisation tools will not be built." >&2;}
	   fi
	fi
	if test -n $JRE_HOME; then
	   { $as_echo "$as_me:${as_lineno-$LINENO}: Using JRE in $JRE_HOME" >&5
$as_echo "$as_me: Using JRE in $JRE_HOME" >&6;}
	   ALL_VISUALISATION=all_visualisation
	   { $as_echo "$as_me:${as_lineno-$LINENO}: Using Grappa $GRAPPA_DIR/$GRAPPA_JAR" >&5
$as_echo "$as_me: Using Grappa $GRAPPA_DIR/$GRAPPA_JAR" >&6;}
	fi
    fi

else
    { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Not looking for Grappa because no Java available" >&5
$as_echo "$as_me: WARNING: Not looking for Grappa because no Java available" >&2;}
