if test "$gfd_requested" = "yes"; then
   GECODEDIR=$(ls -d $GECODE_BASEDIR$GECODE_REQUIRED*/$ARCH  2>/dev/null)
   if test -d "$GECODEDIR"; then
      ALL_WITH_GECODE=all_with_gecode
      ALL_CROSS_WITH_GECODE=all_cross_with_gecode
      { $as_echo "$as_me:${as_lineno-$LINENO}: Using Gecode in $GECODEDIR" >&5
$as_echo "$as_me: Using Gecode in $GECODEDIR" >&6;}
      { $as_echo "$as_me:${as_lineno-$LINENO}: Will build Gecode Interface for Gecode version $GECODE_REQUIRED" >&5
$as_echo "$as_me: Will build Gecode Interface for Gecode version $GECODE_REQUIRED" >&6;}
   else
# try a specific version of Gecode
      GECODEDIR=$(ls -d $GECODE_BASEDIR$GECODE_VERSION/$ARCH  2>/dev/null)
      if test -d "$GECODEDIR"; then
         ALL_WITH_GECODE=all_with_gecode
         ALL_CROSS_WITH_GECODE=all_cross_with_gecode
         { $as_echo "$as_me:${as_lineno-$LINENO}: Using Gecode in $GECODEDIR" >&5
$as_echo "$as_me: Using Gecode in $GECODEDIR" >&6;}
         { $as_echo "$as_me:${as_lineno-$LINENO}: Will build Gecode Interface for Gecode version $GECODE_REQUIRED" >&5
$as_echo "$as_me: Will build Gecode Interface for Gecode version $GECODE_REQUIRED" >&6;}
     fi
   fi
fi
