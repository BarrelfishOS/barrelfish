[
    build library {
        target = "libc_stdtime",
        cFiles = concat [ find cInDir "." ],
        addIncludes = [ "../include", "/lib/tzcode/stdtime", "../locale" ],
        omitCFlags = [ "-std=c99", "-Wmissing-prototypes", "-Wmissing-declarations", "-Wimplicit-function-declaration", "-Werror" ]
    }
]
